	INSTALLER.ERROR=Ceci est une boîte de dialogue d'erreur.

		+=
		+=Quelque chose de vraiment, vraiment, vraiment mauvais est arrivé. Désolé.

	INSTALLER.ERROR.EXIT=
	INSTALLER.ERROR.FRAME.BUTTON.CLOSE=
	INSTALLER.ERROR.FRAME.TITLE=
