	INSTALLER.OPTIONS.LAUNCH.DROP.LIST.FRAME.BUTTON.CLOSE=
	INSTALLER.OPTIONS.LAUNCH.DROP.LIST.FRAME.TITLE=
	INSTALLER.OPTIONS.LAUNCH.DROP.LIST.LIST=
	INSTALLER.OPTIONS.LAUNCH.DROP.LIST.LIST.VSCROLL=
	INSTALLER.OPTIONS.PATH=
	INSTALLER.OPTIONS.PATH.EDIT=
	INSTALLER.THANKS=Ceci est la boîte de fin de l'installeur.

		+=
		+=Elle affiche que l'installation est terminée est vous remercie d'avoir
		+=choisi PGME. Pourquoi avez-vous eu besoin d'aide sur cette boîte ?

	INSTALLER.THANKS.EXIT=
	INSTALLER.THANKS.FRAME.BUTTON.CLOSE=
	INSTALLER.THANKS.FRAME.TITLE=
	INSTALLER.WELCOME=Ceci est la boîte de dialogue de bienvenue de l'installeur.

		+=
		+=Elle affiche la quantité d'espace requise pour une installation complète.

	INSTALLER.WELCOME.CONTINUE=
